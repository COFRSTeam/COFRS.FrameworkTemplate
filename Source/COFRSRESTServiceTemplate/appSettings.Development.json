{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Verbose",
			"Override": {
				"Microsoft": "Warning",
				"System": "Warning",
				"COFRS": "Information"
			}
		}
	},
	"ApiSettings": {
		"RootUrl": "https://$safeprojectname$.development.com",
		"QueryLimit": 50,
		"CacheLimit": 10000,
		"Timeout": "00:20:00",
		"AllowedCors": "*"
	},
	"ConnectionStrings": {
		"DefaultConnection": "Server=developmentdb;Database=master;Trusted_Connection=True;",
		$if$ ($securitymodel$ == OAuth)},
	"OAuth2": {
		"AuthorityURL": "https://IdentityServer.com"
	$endif$}
}
