{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Error",
				"System": "Error",
				"COFRS": "Error"
			}
		}
	},
	"ApiSettings": {
		"RootUrl": "https://$safeprojectname$.staging.com",
		"QueryLimit": 50,
		"Timeout": "00:00:03",
		"AllowedCors": "*"
	},
	"ConnectionStrings": {
		"DefaultConnection": "Server=stagingdb;Database=master;Trusted_Connection=True;"
	$if$ ($securitymodel$ == OAuth)},
	"OAuth2": {
		"AuthorityURL": "https://IdentityServer.com"
	$endif$}
}
